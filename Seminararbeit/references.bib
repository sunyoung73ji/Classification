@article{Ashenfelter2013,
abstract = {In this paper, we use unique data from the market for Bordeaux wine to test the hypothesis that consumers are willing to pay for expert opinion because it is accurate. Using proprietary indicators of the quality of the vintage, which are based on both publicly and privately available information, we find that additional publicly available information on the weather improves the expert's predictions of subsequent prices. This establishes that the expert opinions are not efficient, in the sense that they can be easily improved, and that these opinions must be demanded, at least in part, for some purpose other than their accuracy. (JEL Classification: D8, Q13)},
author = {Ashenfelter, Orley and Jones, Gregory V.},
doi = {10.1017/jwe.2013.22},
file = {:home/jonathan/Dokumente/Mendeley Desktop/Ashenfelter, Jones - 2013 - The Demand for Expert Opinion Bordeaux Wine.pdf:pdf},
issn = {1931-4361},
journal = {Journal of Wine Economics},
number = {3},
pages = {285--293},
title = {{The Demand for Expert Opinion: Bordeaux Wine}},
volume = {8},
year = {2013}
}

