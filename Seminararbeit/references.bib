@article{Hilger2011,
author = {Hilger, James and Rafert, Greg and Villas-Boas, Sofia},
title = {Expert Opinion and the Demand for Experience Goods: An Experimental Approach in the Retail Wine Market},
journal = {The Review of Economics and Statistics},
volume = {93},
number = {4},
pages = {1289-1296},
year = {2011},
doi = {10.1162/REST\_a\_00117},
URL = {https://doi.org/10.1162/REST_a_00117},
eprint = {https://doi.org/10.1162/REST_a_00117},
abstract = {Abstract The effect of expert opinion on demand for experience goods is difficult to quantify, as the relationship between purchases and reviews may be driven by product quality. Further, it is unclear whether a review-based demand effect is due to providing quality or existence information. Using a retail field experiment to overcome these obstacles, we find a significant positive average consumer response to expert opinion labels for wine. Demand decreases for low-scoring wines and increases for wines scoring average or higher. Results indicate that expert opinion labels transmit quality information as opposed to solely shelf visibility.}
}

